@model LoginViewModel
@{
    ViewData["Title"] = "Login";
}

<h2>Login</h2>

<div class="card" style="width: 450px">
    <div class="card-body">
        <div class="text-center">
            <div style="
                    border: 11px solid #8699e5;
                    color: #2399e5;
                    border-radius: 100%;
                    font-weight: 700;
                    width: 100px;
                    height: 100px;
                    margin: 0 auto .5em auto;
                    font-size: 3em;">
            </div>
            <h4 style="color: #2399e5; font-weight: 700;">Test</h4>
            <p>
                Please enter your username and password
            </p>
        </div>

    </div>
</div>

<ul>
    @foreach (var val in Model.Values)
    {
        <li>Value: @val</li>
    }
</ul>